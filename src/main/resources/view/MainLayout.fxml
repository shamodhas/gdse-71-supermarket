<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.gdse.supermarketfx.controller.MainLayoutController">
   <children>
      <Pane prefHeight="500.0" prefWidth="200.0" style="-fx-background-color: #7367F0;">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="500.0" prefWidth="200.0" spacing="50.0">
               <children>
                  <Label text="Super Market" textFill="WHITE">
                     <font>
                        <Font name="Calibri" size="24.0" />
                     </font>
                  </Label>
                  <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
                     <children>
                        <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#navigateCustomerOnAction" style="-fx-background-color: #7367F0;" text="Customer" textFill="WHITE">
                           <font>
                              <Font name="Calibri" size="18.0" />
                           </font>
                        </Button>
                        <Button layoutX="10.0" layoutY="10.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#navigateItemOnAction" style="-fx-background-color: #7367F0;" text="Item" textFill="WHITE">
                           <font>
                              <Font name="Calibri" size="18.0" />
                           </font>
                        </Button>
                        <Button layoutX="10.0" layoutY="56.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#navigateOrdersOnAction" style="-fx-background-color: #7367F0;" text="Orders" textFill="WHITE">
                           <font>
                              <Font name="Calibri" size="18.0" />
                           </font>
                        </Button>
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </VBox>
               </children>
               <padding>
                  <Insets top="20.0" />
               </padding>
            </VBox>
         </children>
      </Pane>
      <AnchorPane fx:id="content" layoutX="200.0" prefHeight="500.0" prefWidth="600.0" style="-fx-background-color: #000;" />
   </children>
</AnchorPane>
